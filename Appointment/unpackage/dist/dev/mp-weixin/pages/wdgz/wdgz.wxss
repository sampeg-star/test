page {
  background-color: #F1F1F1;
  padding-bottom: 50rpx;
}
.lljl_nr {
  width: 85%;
  margin: 30rpx auto;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 40rpx 30rpx 40rpx 30rpx;
  border-radius: 10px;
  background-color: #FFFFFF;
}
.lljl_nr image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 10px;
}
.lljl_nr .lljl_text {
  width: 430rpx;
}
.lljl_nr .lljl_text .lljl_name {
  font-size: 35rpx;
  margin-bottom: 20rpx;
}
.lljl_nr .lljl_text .lljl_msg {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  font-size: 28rpx;
  color: #C8C7CC;
  margin-bottom: 10rpx;
}
.lljl_nr .lljl_text .lljl_msg .sex {
  color: #ff7b72;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.lljl_nr .lljl_text .lljl_msg image {
  width: 40rpx;
  height: 40rpx;
}
.lljl_nr .lljl_text .lljl_tap {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 28rpx;
}
.lljl_nr .lljl_text .lljl_tap .lljl_btn {
  width: 180rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  color: #FFFFFF;
  border-radius: 50px;
  background-image: -webkit-linear-gradient(left, #ff542d, #ff8f46, #ff9548);
  background-image: linear-gradient(to right, #ff542d, #ff8f46, #ff9548);
}

